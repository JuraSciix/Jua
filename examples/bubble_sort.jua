/** Алгоритм сортировки пузырьком. Сложность: O(N^2) */
fn bubble_sort(a) { // Реальная сложность примерно равна N(N-1)/2
    var len = length(a);
    for var i = 0; i < len; i++ {
        // Все элементы до i уже стоят на соответствующих местах,
        // и не могут быть меньше текущего.
        for var j = 1; j < len - i; j++ {
            var x = a[j - 1];
            var y = a[j];
            if x > y {
                a[j - 1] = y;
                a[j] = x;
            }
        }
    }
}