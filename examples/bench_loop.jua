fn test_loop() {
    for var i = 0; i < 1_000; i++ {
        for var j = 0; j < 1_000; j++ {
            // no-op
        }
    }
}

fn bench_loop(itrc = 5) {
    for var itr = 1; itr <= itrc; itr++ {
        var m_start = ns_time();
        test_loop();
        var m_end = ns_time();

        print_bench_data({
            'name': 'Loop bench',
            'score': m_end - m_start,
            'i':    itr,
            'c':    itrc
        });
    }
}