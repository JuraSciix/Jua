fn test_loop() {
    for i = 0; i < 1_000; i++ {
        for j = 0; j < 1_000; j++ {
            // no-op
        }
    }
}

fn bench_loop(itrc = 5) {
    for itr = 1; itr <= itrc; itr++ {
        m_start = ns_time();
        test_loop();
        m_end = ns_time();

        print_bench_data({
            'name': 'Loop bench',
            'score': m_end - m_start,
            'i':    itr,
            'c':    itrc
        });
    }
}