// Медленная, рекурсивная реализация алгоритма Фиббоначи
fn fib_slow(n) = (n <= 2) ? 1 : fib_slow(n - 1) + fib_slow(n - 2);

// Быстрая, линейная, итеративная реализация алгоритма Фиббоначи
fn fib_fast(n) {
    var q1 = 1, q2 = 1;
    for var i = 2; i < n; i++ {
        var qt = q1 + q2;
        q1 = q2;
        q2 = qt;
    }
    return q2;
}