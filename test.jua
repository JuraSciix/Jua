const _NaN = 0.0 / 0.0;

fn fib(n) {
    if n <= 2 {
        if n <= 0 {
            return _NaN;
        }
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

fn qfib(n) {
    if n <= 0 {
        return _NaN;
    }
    q1 = 1;
    q2 = 1;
    for i = 2; i < n; i++ {
        qt = q1 + q2;
        q1 = q2;
        q2 = qt;
    }
    return q2;
}

fn recursion_test(n = 1, t = 0) {
    if n % 1000 == 0 {
        println(n + ' (' + (ns_time() - t) / 1E6 + 'ms)');
        t = ns_time();
    }
    return recursion_test(n + 1, t);
}

println(fib(11));
println(qfib(11));