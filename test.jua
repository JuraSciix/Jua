fn main(args) {
    if args[0] == "--test" {
        for var i = 0; i < 100; i++ {
            if (i & 8) != 0 {
                continue;
            }
            test(i);
        }
    } else {
        println("No test");
    }
}

fn test(i) {
    while i-- > 0 {
        switch i & 8 {
            case 1, 2: println("1 || 2");
            case 3, 4: println("3 || 4");
            case 5: fallthrough;
            default: println("5 || <unknown>");
        }
    }
}